{
    "version": 2,
    "name": "Full Stack",
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "redirects": [
        {
            "source": "/(.*)",
            "destination": "https://your-domain.com/$1",
            "statusCode": 301
        }
    ]
}
